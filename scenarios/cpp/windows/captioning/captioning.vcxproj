    </PropertyGroup>
    <Error Condition="!Exists('..\packages\Microsoft.CognitiveServices.Speech.1.21.0\build\native\Microsoft.CognitiveServices.Speech.targets')" Text="$([System.String]::Format('$(ErrorText)', '..\packages\Microsoft.CognitiveServices.Speech.1.21.0\build\native\Microsoft.CognitiveServices.Speech.targets'))" />
